{
    "name": "ippey/gh-random-contributor",
    "description": "This is a library for getting random GitHub contributors.",
    "type": "library",
    "require": {
        "symfony/http-client": "^5.3",
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.91",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ippey\\GhRandomContributor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ippey\\GhRandomContributor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit tests",
        "cs-fix": "@php vendor/bin/php-cs-fixer fix",
        "cs": "@php vendor/bin/php-cs-fixer fix -v --dry-run"
    },
    "authors": [
        {
            "name": "Ippei Sumida",
            "email": "ippey.s@gmail.com"
        }
    ]
}
